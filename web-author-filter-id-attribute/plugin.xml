<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">

<plugin
    id="web-author-filter-id-attribute"
    name="web-author-filter-id-attribute"
    description="A plugin for oXygen XML Web Author that filters ID attribute on any element."
    version="1.0"
    vendor="Syncro Soft">

  <runtime>
    <librariesFolder name="lib" />
    <library name="target/classes" />
    <librariesFolder name="target/lib" />
  </runtime>

  <extension type="WorkspaceAccess" class="com.oxygenxml.filter.attribute.CustomWorkspaceAccessPluginExtension"/>
</plugin>
